		"dexie": "^4.2.0",
		"lucide-react": "^0.400.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-dropzone": "^14.2.9",
		"react-hook-form": "^7.51.3",
		"react-router-dom": "^6.26.1",
		"recharts": "^2.12.7",
		"zustand": "^4.5.2"
	}
